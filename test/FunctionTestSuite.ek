@start noArgs
import <test>

func doSomething()
{
    int a = 3;
}

doSomething();
@end


@start increment
import <test>
func increment(int a)
{
    a = a + 1;
}
int a = 2;
increment(a);
ASSERT(a == 2);
@end

@start returnInt
import <test>

func increment(int a)
{
    return (a + 1);
}

int a = 2;
a = increment(a);
ASSERT(a == 3);
@end


