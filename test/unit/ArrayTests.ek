import <io>
import <test>

{
    array test = [1, 2, 3];
    TEST(test[0] == 1 && test[1] == 2, "array access");

    int index = 0;
    TEST(test[index] == 1, "index using variable");

    int val = test[0];
    TEST(val == 1 && test[0] == 1, "assign to variable");

    val = 2;
    TEST(val == 2 && test[0] == 1, "copy array value.");

    test[0] = test[1];
    TEST(test[0] == 2, "set array int value.");
}
