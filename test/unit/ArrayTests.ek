import <io>
import <test>

print("Running Array tests.");


func arrayAccessTestCase()
{
    array test = [1, 2, 3];
    TEST(test[0] == 1 && test[1] == 2, "array access");
}


func arrayModificationTestCase()
{
    array intArray = [1, 2, 3];
    TEST(intArray[0] == 1 && intArray[1] == 2, "array indexing");

    int index = 0;
    TEST(intArray[index] == 1, "index using variable");

    //test[0] = 100;
    //TEST(test[0] == 100 && test[1] == 2, "array modification");
}

